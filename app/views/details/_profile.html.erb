<h2 align='center'> Profile</h2>
<div class="row">
    <div class="well cv col-xs-8 col-offset-2">
        <div class='col-xs-2 cv-logo'>
        </div>
        <div class='col-xs-12'>
            <div class="article-body ">
                <%= obj.profile  %>
            </div>
            <hr>
            <div style="display:'flex' flex-wrap:'rows'">
                <div class="center ">
                    <% if obj.linkedin != '' %>
                        <a href="<%= obj.linkedin %>">
                            <img src="/assets/linkedin.png", alt="linkedin", class="image-link"/>
                        </a>
                    <% end %>
                    <% if obj.github != '' %>
                        <a href="<%= obj.github %>">
                            <img src="/assets/github.png", alt="github", class="image-link"/>
                        </a>
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>